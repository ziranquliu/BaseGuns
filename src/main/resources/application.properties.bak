guns.swagger-open=true
guns.kaptcha-open=false
guns.session-open=false
guns.muti-datasource-open=false
guns.spring-session-open=false
guns.session-invalidate-time=1800
guns.session-validation-interval=900
server.port=8080
mybatis.type-aliases-package=com.stylefeng.guns.common.persistence.model
mybatis.mapper-locations=classpath:com/stylefeng/guns/**/*.xml
pagehelper.helperDialect=mysql
pagehelper.reasonable=true
pagehelper.supportMethodsArguments=true
pagehelper.params=count=countSql
beetl.delimiter-statement-start=\@
beetl.delimiter-statement-end=null
beetl.resource-tagroot=common/tags
beetl.resource-tagsuffix=tag
beetl.resource-auto-check=true
spring.redis.host=localhost
spring.redis.port=6379
spring.redis.password=
spring.profiles.active=development
spring.mvc.static-path-pattern=/static/**
spring.mvc.view.prefix=/WEB-INF/view
spring.http.converters.preferred-json-mapper=fastjson
spring.http.multipart.max-request-size=100MB
spring.http.multipart.max-file-size=100MB
spring.devtools.restart.enabled=true
spring.devtools.restart.additional-paths=src/main/java
spring.devtools.restart.exclude=static/**,WEB-INF/view/**
spring.profiles=development
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/guns?autoReconnect=true&useUnicode=true&characterEncoding=UTF-8&zeroDateTimeBehavior=CONVERT_TO_NULL&allowMultiQueries=true&useSSL=false&serverTimezone=GMT%2B8
spring.datasource.username=root
spring.datasource.password=Hello1234
spring.datasource.filters=log4j,wall,mergeStat
biz.datasource.url=jdbc:mysql://127.0.0.1:3306/biz?autoReconnect=true&useUnicode=true&characterEncoding=UTF-8&zeroDateTimeBehavior=CONVERT_TO_NULL&allowMultiQueries=true&useSSL=false&serverTimezone=GMT%2B8
biz.datasource.username=root
biz.datasource.password=Hello1234
logging.level.root=info
logging.level.com.stylefeng=debug
logging.path=logs/
logging.file=guns.log
spring.profiles=produce
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/guns?autoReconnect=true&useUnicode=true&characterEncoding=UTF-8&zeroDateTimeBehavior=CONVERT_TO_NULL&allowMultiQueries=true&useSSL=false&serverTimezone=GMT%2B8
spring.datasource.username=root
spring.datasource.password=Hello1234
spring.datasource.filters=log4j,wall,mergeStat
logging.level.root=warn
logging.path=logs/
logging.file=guns.log